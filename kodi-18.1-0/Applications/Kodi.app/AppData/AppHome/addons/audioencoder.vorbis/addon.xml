<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="audioencoder.vorbis"
  version="2.0.1"
  name="Vorbis Audio Encoder"
  provider-name="spiff">
  <requires>
<import addon="kodi.binary.global.main" version="1.0.14"/>
<import addon="kodi.binary.instance.audioencoder" version="2.0.0"/></requires>
  <extension
    point="kodi.audioencoder"
    extension=".ogg"
    library_ios="audioencoder.vorbis.2.0.1.dylib"/>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Vorbis Audio Encoder</summary>
    <description lang="en">Vorbis is a general purpose perceptual audio CODEC intended to allow maximum encoder flexibility, thus allowing it to scale competitively over an exceptionally wide range of bitrates.</description>
    <platform>ios-aarch64</platform>
  </extension>
</addon>
